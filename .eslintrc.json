{
  "extends": ["next/core-web-vitals", "@rocketseat/eslint-config/react"],
  "rules": {
    "prettier/prettier":[ "error",
    {
      "singleQuote": true,
			"jsxSingleQuote": true,
			"semi": true,
			"bracketSpacing": true,
			"trailingComma": "none",
      "endOfLine": "auto"
			}],
      "no-undef": "off",
			"no-unused-vars": "error"
  }
}
